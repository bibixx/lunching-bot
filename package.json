{
  "name": "lunching-bot",
  "version": "1.0.0",
  "private": true,
  "description": "",
  "scripts": {
    "serve": "node -r ts-node/register/transpile-only -r tsconfig-paths/register dist/index.js",
    "serve:dev": "nodemon -r ts-node/register/transpile-only -r tsconfig-paths/register dist/index.js",
    "build": "tsc -p tsconfig.json",
    "dev": "concurrently \"yarn serve:dev\" \"yarn build -w\"",
    "start": "yarn build && yarn serve",
    "tslint": "tslint -c tslint.json -p tsconfig.json"
  },
  "dependencies": {
    "@types/dotenv": "^6.1.0",
    "@types/express": "^4.16.0",
    "@types/node": "^10.12.18",
    "@types/pg": "^7.4.11",
    "concurrently": "^4.1.0",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "pg": "^7.8.0",
    "ts-node": "^7.0.1",
    "tsconfig-paths": "^3.7.0",
    "tslint": "^5.12.1",
    "tslint-config-airbnb": "^5.11.1",
    "typescript": "^3.2.2"
  },
  "devDependencies": {
    "nodemon": "^1.18.9"
  }
}
